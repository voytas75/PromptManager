    def setSpacing(self, spacing: int) -> None: ...
    def addWidget(self, widget: QWidget, stretch: int | None = None) -> None: ...
    def addLayout(self, layout: QLayout, stretch: int | None = None) -> None: ...
    def addStretch(self, stretch: int = ...) -> None: ...

